    "validateUpsert": true
  },
  "ttl": 3600,
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "scoreId": {
      "type": "array"
    },
    "password": {
      "type": "object",
      "required": true
    },
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "modifiedon": {
      "type": "date",
      "required": true
    },
    "registeredon": {
      "type": "date",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
